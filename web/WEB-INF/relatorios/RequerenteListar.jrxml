	<queryString language="SQL">
		<![CDATA[SELECT
     `id_requerente` AS Código,
	 CONCAT_WS(' ',nome_requerente,sobrenome_requerente) AS Nome,
     CONCAT_WS('/',telefone_principal,telefone_alternativo_requerente) AS Telefone,
     email_principal_requerente AS Emails,
     `home_page_requerente` AS 'Home Page',
     `sexo_requerente` AS Sexo,
     `data_nascimento` AS data_nascimento,
     `categoria_juridica` AS categoria_juridica
FROM
     `categoria_juridica` CJ INNER JOIN `requerente` R ON CJ.`id_categoria_juridica` = R.`id_categoria_juridica`]]>
	</queryString>
