	<parameter name="id_movimento_progressivo" class="java.lang.Integer">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[SELECT
     `id_movimento_progressivo` AS id_movimento_progressivo,
     `data_recepcao` AS data_recepcao,
     `data_reenvio` AS data_reenvio,
     `departamento` AS departamento,
     `origem` AS origem,
     `descricao_assunto` AS descricao_assunto,
     `notas` AS notas
FROM
     `departamento` D INNER JOIN `movimento_documento` MD ON D.`id_departamento` = MD.`id_departamento`
     INNER JOIN `documento` Doc ON MD.`numero_protocolo` = Doc.`numero_protocolo`
WHERE
     id_movimento_progressivo = $P{id_movimento_progressivo}]]>
	</queryString>
