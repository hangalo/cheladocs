	<parameter name="codigo_documento" class="java.lang.Integer">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[SELECT
     `numero_protocolo` AS numero_protocolo,
     `data_entrada` AS data_entrada,
     `origem` AS origem,
     `descricao_assunto` AS assunto,
     CONCAT_WS(' ',nome_requerente,sobrenome_requerente) AS Requerente,
     natureza_assunto AS natureza_assunto,
     TE.`tipo_expediente` AS t_expediente
FROM
     `tipo_expediente` TE INNER JOIN `documento` D ON TE.`id_tipo_expediente` = D.`id_tipo_expediente`
     INNER JOIN `requerente` R ON D.`id_requerente` = R.`id_requerente`
     INNER JOIN `natureza_assunto` NA ON D.`id_natureza_assunto` = NA.`id_natureza_assunto`
WHERE
     numero_protocolo = $P{codigo_documento}]]>
	</queryString>
